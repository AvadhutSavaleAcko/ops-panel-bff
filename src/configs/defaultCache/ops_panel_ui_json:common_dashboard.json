{
  "url": {
    "base_path": "ops_panel/dashboard",
    "host_name": "",
    "query_params": {}
  },
  "actions": {
    "on_load": [
      {
        "args": {
          "payloadData": {},
          "requestBody": {
            "journey": "ops_panel",
            "current_node": "common_dashboard"
          }
        },
        "type": "setRequestPayload"
      }
    ]
  },
  "children": [
    {
      "id": "dashboard_header",
      "type": "header",
      "data": {
        "row": 1,
        "props": {
          "is_visible": true,
          "productOptions": [
            {
              "value": "auto",
              "displayValue": "Auto"
            }
          ],
          "defaultProduct": {
            "value": "auto",
            "displayValue": "Auto"
          }
        }
      }
    },
    {
      "id": "dashboard_layout",
      "type": "flex_container",
      "data": {
        "props": {
          "is_visible": true,
          "styles": {
            "display": "flex",
            "min-height": "calc(100vh - 80px)"
          }
        },
        "column": 2
      },
      "children": [
        {
          "id": "sidebar_container",
          "type": "sidebaar_container",
          "data": {
            "props": {
              "is_visible": true,
              "styles": {
                "width": "250px",
                "background": "#2c3e50"
              }
            },
            "column": 1
          },
          "children": [
            {
              "id": "navigation_tabs",
              "type": "navigation_container",
              "data": {
                "props": {
                  "is_visible": true,
                  "showForm": false,
                  "activeTab": "",
                  "activeTabData": [],
                  "dataToConfigureNewModel": "#dataToConfigureNewModel"
                },
                "iterableKey": "tab_name_list"
              },
              "children": [
                {
                  "id": "nav_item",
                  "type": "navigation",
                  "data": {
                    "props": {
                      "is_visible": true,
                      "tab_name": "#tab_name"
                    }
                  },
                  "actions": {
                    "on_click": [
                      {
                        "type": "submit_call",
                        "args": {
                          "ids": [
                            "content_title",
                            "data_table"
                          ],
                          "reverseIds": [
                            "add_model_form",
                            "content_title"
                          ]
                        }
                      }
                    ]
                  }
                }
              ]
            },
            {
              "id": "add_model_button",
              "type": "add_model_button",
              "data": {
                "props": {
                  "is_visible": true,
                  "label": "+ Add Model"
                }
              },
              "actions": {
                "on_click": [
                  {
                    "type": "submit_call",
                    "args": {
                      "ids": [
                        "add_model_form"
                      ],
                      "reverseIds": [
                        "data_table",
                        "configure_model",
                        "content_title"
                      ]
                    }
                  }
                ]
              }
            }
          ]
        },
        {
          "id": "content_section",
          "type": "box_container",
          "data": {
            "props": {
              "is_visible": true,
              "styles": {
                "flex": "1",
                "padding": "24px",
                "margin-left": "40px"
              }
            },
            "column": 2
          },
          "children": [
            {
              "id": "content_title",
              "type": "page_heading",
              "data": {
                "row": 1,
                "props": {
                  "is_visible": false,
                  "text": "proposals"
                }
              }
            },
            {
              "id": "data_table",
              "type": "default_table",
              "data": {
                "row": 2,
                "props": {
                  "is_visible": false,
                  "data": []
                }
              },
              "actions": {
                "view_details": [
                  {
                    "type": "updateProps",
                    "args": {
                      "component_id": "proposal_json_modal",
                      "update_props": {
                        "is_visible": true,
                        "jsonData": ""
                      }
                    }
                  }
                ]
              }
            },
            {
              "id": "add_model_form",
              "type": "add_model_form",
              "data": {
                "props": {
                  "row": 3,
                  "is_visible": true,
                  "heading": "Add New Model",
                  "formFields": [
                    {
                      "id": "tab_name_field",
                      "type": "inputfield",
                      "data": {
                        "props": {
                          "name": "tab_name",
                          "type": "text",
                          "label": "navigation Tab Name",
                          "required": true,
                          "placeholder": "Enter tab name"
                        }
                      }
                    },
                    {
                      "id": "row_field",
                      "type": "inputfield",
                      "data": {
                        "props": {
                          "name": "row",
                          "type": "number",
                          "label": "Row",
                          "required": true,
                          "placeholder": "Enter row number"
                        }
                      }
                    },
                    {
                      "id": "url_field",
                      "type": "inputfield",
                      "data": {
                        "props": {
                          "name": "url",
                          "type": "text",
                          "label": "URL",
                          "required": true,
                          "placeholder": "Enter URL"
                        }
                      }
                    },
                    {
                      "id": "method_field",
                      "type": "dropdown",
                      "data": {
                        "props": {
                          "name": "method",
                          "label": "Method",
                          "options": [
                            {
                              "value": "GET",
                              "label": "GET"
                            },
                            {
                              "value": "POST",
                              "label": "POST"
                            },
                            {
                              "value": "PUT",
                              "label": "PUT"
                            },
                            {
                              "value": "DELETE",
                              "label": "DELETE"
                            }
                          ]
                        }
                      }
                    }
                  ],
                  "keyValueSections": [
                    {
                      "id": "path_variables",
                      "type": "key_value_section",
                      "data": {
                        "props": {
                          "title": "Path Variables",
                          "fieldName": "path_variable"
                        }
                      }
                    },
                    {
                      "id": "headers",
                      "type": "key_value_section",
                      "data": {
                        "props": {
                          "title": "Headers",
                          "fieldName": "headers"
                        }
                      }
                    },
                    {
                      "id": "params",
                      "type": "key_value_section",
                      "data": {
                        "props": {
                          "title": "Parameters",
                          "fieldName": "params"
                        }
                      }
                    },
                    {
                      "id": "body",
                      "type": "key_value_section",
                      "data": {
                        "props": {
                          "title": "Body Fields",
                          "fieldName": "body"
                        }
                      }
                    }
                  ]
                }
              },
              "actions": {
                "on_submit": [
                  {
                    "type": "submitModelForm",
                    "args": {
                      "requestBody": {
                        "current_node": "common_dashboard",
                        "journey": "ops_panel"
                      }
                    }
                  }
                ],
                "on_cancel": [
                  {
                    "type": "setGlobalData",
                    "args": {
                      "showForm": false
                    }
                  }
                ]
              }
            },
            {
              "id": "configure_model",
              "type": "configure_new_model",
              "data": {
                "row": 4,
                "props": {
                  "is_visible": false,
                  "data": "dataToConfigureNewModel"
                }
              },
              "actions": {
                "on_save": [
                  {
                    "type": "fetch_config_redirect",
                    "args": {
                      "shouldRedirect": true,
                      "showBasicLoader": true
                    }
                  }
                ]
              }
            }
          ]
        }
      ]
    }
  ],
  "meta_data": {
    "link": "",
    "page_title": "default list",
    "description": "Operations Panel default list for tabs"
  },
  "current_node": "common_dashboard"
}